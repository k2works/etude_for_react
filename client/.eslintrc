{
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "rules": {
    "react/jsx-wrap-multilines": 0,
    "eol-last": 0,
    "function-paren-newline": 0,
    "jsx-a11y/no-redundant-roles": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/jsx-filename-extension": 0
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "polyfills": ["fetch"]
  }
}